# Cecil Environment Variables
# Copy this file to .env and fill in your values.
# NEVER commit the .env file — it is gitignored.

# Logging
CECIL_LOG_LEVEL=INFO

# Server
CECIL_PORT=8787

# AWS (optional — only needed for AWS CloudWatch provider)
CECIL_AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=

# Execution mode: ai_optimization | generic | audit
CECIL_MODE=ai_optimization
